<h1>New Employee Form</h1>

<ul>
<%=form_for @employee do |f|%>
<%=f.label :first_name%>
<%=f.text_field :first_name%>

<%=f.label :last_name%>
<%=f.text_field :last_name%>

<%=f.label :alias%>
<%=f.text_field :alias%>

<%=f.label :title%>
<%=f.text_field :title %>

<%=f.label :office%>
<%=f.collection_select :office, Employee.all, :office, :office%>

<%=f.label :dog_id%>
<%=f.collection_select :dog_id, Dog.all, :id, :name%>


</ul>



<%=f.submit%>


<%end%>

<%if flash[:messages]%>
<%= flash[:messages]%>
<%end%>